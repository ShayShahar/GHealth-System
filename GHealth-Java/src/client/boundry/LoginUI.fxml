<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="379.0" prefWidth="486.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.control.UserController">
   <children>
      <Button fx:id="loginBtn" layoutX="205.0" layoutY="255.0" mnemonicParsing="false" onAction="#onLoginButtonClick" prefHeight="42.0" prefWidth="95.0" text="Login">
         <effect>
            <Glow />
         </effect>
         <font>
            <Font name="Segoe UI" size="12.0" />
         </font></Button>
      <ImageView fx:id="footer_img" fitHeight="173.0" fitWidth="294.0" layoutX="106.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../img/header.png" />
         </image>
      </ImageView>
      <Separator layoutX="153.0" layoutY="312.0" prefWidth="200.0" />
      <TextField fx:id="userNameTxt" layoutX="178.0" layoutY="167.0" prefHeight="31.0" prefWidth="149.0" promptText="Username" />
      <TextField fx:id="passwordTxt" layoutX="178.0" layoutY="209.0" prefHeight="31.0" prefWidth="149.0" promptText="Password" />
      <Separator layoutX="153.0" layoutY="143.0" prefWidth="200.0" />
      <Button fx:id="settingsBtn" layoutX="423.0" layoutY="333.0" mnemonicParsing="false" onAction="#onSettingsButtonClick" prefHeight="25.0" prefWidth="36.0" />
      <ImageView fx:id="settingsImg" fitHeight="25.0" fitWidth="21.0" focusTraversable="true" layoutX="431.0" layoutY="335.0" opacity="0.37" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../img/settings_icon.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
