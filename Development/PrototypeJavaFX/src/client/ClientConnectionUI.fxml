<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="379.0" prefWidth="486.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ClientConnectionController">
   <children>
      <Button fx:id="connectBtn" layoutX="218.0" layoutY="237.0" mnemonicParsing="false" onAction="#onConnectButtonClick" prefHeight="42.0" prefWidth="95.0" text="Connect">
         <effect>
            <Glow />
         </effect>
         <font>
            <Font name="Segoe UI" size="12.0" />
         </font></Button>
      <TextField fx:id="ipAddress" layoutX="166.0" layoutY="173.0" prefHeight="42.0" prefWidth="200.0" promptText="IP Address" />
      <ImageView fx:id="header_img" fitHeight="145.0" fitWidth="375.0" layoutX="117.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="img/header.png" />
         </image>
      </ImageView>
      <ImageView fx:id="footer_img" fitHeight="150.0" fitWidth="200.0" layoutX="304.0" layoutY="290.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="img/client_footer.png" />
         </image>
      </ImageView>
      <Separator layoutX="166.0" layoutY="312.0" prefWidth="200.0" />
   </children>
</AnchorPane>
